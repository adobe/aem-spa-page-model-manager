{
  "name": "@adobe/aem-spa-page-model-manager",
  "version": "1.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aem-spa-page-model-manager.git"
  },
  "bugs": {
    "url": "https://www.adobe.com/go/aem6_4_support_en"
  },
  "engines": {
    "npm": ">=5.5.1"
  },
  "files": [
    "dist/"
  ],
  "homepage": "https://www.adobe.com/go/aem6_4_docs_spa_en",
  "keywords": [
    "the official",
    "adobe",
    "aem",
    "spa editor",
    "experience manager",
    "marketing cloud",
    "cq",
    "reactjs",
    "pagemodelmanager"
  ],
  "main": "dist/aem-spa-page-model-manager.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "clean": "rm -rf coverage/ dist/",
    "linter": "eslint --ext .js,.ts . && tslint --project tsconfig.json",
    "linter:fix": "eslint --ext .js,.jsx . && tslint --project tsconfig.json --fix",
    "test": "jest --clearCache && jest",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --coverage --watchAll",
    "build": "npm run clean && npm run linter && cross-env NODE_ENV=development webpack && npm run build:types",
    "build:production": "npm run clean && npm run linter && cross-env NODE_ENV=production webpack --mode=production && npm run build:types",
    "build:types": "tsc -p src/tsconfig.types.json",
    "preversion": "npm run test && npm run build:production",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "semantic-release": "semantic-release"
  },
  "author": "Adobe Systems Inc.",
  "license": "Apache-2.0",
  "dependencies": {
    "clone": "^2.1.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/user-event": "^12.0.1",
    "@types/clone": "^0.1.30",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "fetch-mock": "^8.0.1",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^26.0.1",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-resolve": "^26.0.1",
    "jest-watch-typeahead": "^0.6.0",
    "jsdoc": "^3.6.3",
    "semantic-release": "^17.1.1",
    "node-fetch": "^2.6.0",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-mockito": "^2.6.1",
    "tslint": "^6.1.2",
    "tslint-etc": "^1.11.0",
    "tslint-no-toplevel-property-access": "0.0.2",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.7.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
